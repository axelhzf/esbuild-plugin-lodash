{
  "npm": {
    "publish": true
  },
  "github": {
    "release": true,
    "releaseName": "Release v${version}",
    "tagName": "v${version}",
    "commitMessage": "chore(release): Release v${version}"
  },
  "plugins": {
    "@release-it/keep-a-changelog": {
      "filename": "CHANGELOG.md"
    }
  }
}