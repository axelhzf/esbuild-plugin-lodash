{
  "npm": {
    "publish": true,
    "publishPath": ".",
    "otp": null,
    "ignoreVersion": false,
    "skipChecks": false,
    "timeout": 10
  },
  "github": {
    "release": true,
    "releaseName": "Release v${version}"
  },
  "plugins": {
    "@release-it/keep-a-changelog": {
      "filename": "CHANGELOG.md"
    }
  }
}